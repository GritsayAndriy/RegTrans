<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefHeight="316.0" prefWidth="340.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.regtrans.controller.report.CreateReportController">
   <children>
      <VBox prefHeight="91.0" prefWidth="300.0">
         <children>
            <Label fx:id="labelWhatCreation" text="Створення ">
               <font>
                  <Font size="18.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="labelFullName" text="Label">
               <font>
                  <Font size="15.0" />
               </font>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Label>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
      </VBox>
      <VBox prefHeight="176.0" prefWidth="300.0">
         <children>
            <Label fx:id="labelRoute" text="Маршрут" />
            <TextField fx:id="fieldRoute" />
            <Label fx:id="labelCustomer" text="Замовник" />
            <TextField fx:id="fieldCustomer" />
            <Label fx:id="labelAddress" text="Адреса об'єкта" />
            <TextField fx:id="fieldAddress" />
            <Button fx:id="btnCreateReport" mnemonicParsing="false" onAction="#createReport" text="Створити">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <AnchorPane>
               <children>
                  <Label layoutX="1.0" layoutY="-2.0" text="Документ:" />
                  <Label fx:id="labelCount" layoutX="70.0" layoutY="-2.0" text="Label" />
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" top="20.0" />
               </VBox.margin>
            </AnchorPane>
         </children>
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="5.0" />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
